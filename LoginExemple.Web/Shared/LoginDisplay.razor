@using Microsoft.AspNetCore.Components.Authorization
<AuthorizeView>
    <Authorized>
        <a class="py-2 d-none d-md-inline-block" href="Identity/Account/Manage">@context.User.Identity.Name</a>
        <a class="py-2 d-none d-md-inline-block" href="Identity/Account/Logout">Log out</a>
    </Authorized>
    <NotAuthorized>
        <a class="py-2 d-none d-md-inline-block" href="Identity/Account/Register">Register</a>
        <a class="py-2 d-none d-md-inline-block" href="Identity/Account/Login">Log in</a>
    </NotAuthorized>
</AuthorizeView>

@code{
    private string UserName;

    protected override void OnInitialized()
    {
        base.OnInitialized();

    }
}